from datetime import datetime


# Установим дату 2 декабря 2023 года
start_date = datetime(2023, 12, 2)

# Получим текущую дату
current_date = datetime.now()

# Вычислим разницу между текущей датой и датой начала года
delta = current_date - start_date

# Выведем количество прошедших дней
print("Прошло дней с 2 декабря 2023 года :", delta.days)
